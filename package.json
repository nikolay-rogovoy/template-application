{
  "name": "angular-quickstart",
  "version": "1.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "ngc-aot": "rimraf tmp/aot && ngc -p tsconfig-aot.json",
    "bundle-aot": "rimraf dist/aot && rollup -c rollup-config-aot.js",
    "copy-aot": "node tools/copy-aot.js",
    "build-aot": "npm run ngc-aot && npm run bundle-aot && npm run copy-aot",
    "tsc-jit": "tsc -p tsconfig.json",
    "tsc-jit-w": "tsc -w -p tsconfig.json",
    "serve": "lite-server -c=bs-config.json",
    "start": "concurrently \"npm run tsc-jit-w\" \"npm run serve\"",
    "lint": "tslint ./src/**/*.ts -t verbose"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^4.3.1",
    "@angular/compiler": "^4.3.1",
    "@angular/compiler-cli": "^4.3.1",
    "@angular/core": "^4.3.1",
    "@angular/forms": "^4.3.1",
    "@angular/http": "^4.3.1",
    "@angular/platform-browser": "^4.3.1",
    "@angular/platform-browser-dynamic": "^4.3.1",
    "@angular/platform-server": "^4.3.1",
    "@angular/router": "^4.3.1",
    "angular-in-memory-web-api": "~0.3.0",
    "at-grid": "^1.0.5",
    "commander": "^2.11.0",
    "core-js": "^2.4.1",
    "debug": "^2.6.8",
    "rxjs": "^5.4.2",
    "spawn-command": "0.0.2",
    "systemjs": "^0.20.17",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.53",
    "@types/node": "^8.0.11",
    "canonical-path": "0.0.2",
    "concurrently": "^3.2.0",
    "copy-dir": "^0.3.0",
    "del": "^3.0.0",
    "jasmine-core": "~2.6.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "lite-server": "^2.2.2",
    "lodash": "^4.16.4",
    "mkdirp": "^0.5.1",
    "protractor": "~5.1.2",
    "rimraf": "^2.5.4",
    "rollup": "^0.45.2",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "tslint": "^5.5.0",
    "typescript": "~2.3.3"
  },
  "repository": {}
}
